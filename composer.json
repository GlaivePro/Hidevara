{
    "name": "glaivepro/hidevara",
    "type": "library",
    "description": "Laravel millipackage that hides variables from getting dumped in the Whoops page when your app crashes.",
    "keywords": [
        "Laravel",
        "Whoops",
		"Security"
    ],
    "homepage": "https://github.com/GlaivePro/Hidevara",
    "license": "MIT",
    "authors": [
        {
            "name": "Juris Evertovskis",
            "email": "juris@glaive.pro",
            "homepage": "https://github.com/tontonsb",
            "role": "Developer"
        }
    ],
    "require": {
        "laravel/framework": ">=7.0"
    },
    "autoload": {
        "psr-4": {
            "GlaivePro\\Hidevara\\": "src"
        }
    },
    "extra": {
		"laravel": {
			"providers": [
				"GlaivePro\\Hidevara\\Provider"
			]
		}
    }
}
